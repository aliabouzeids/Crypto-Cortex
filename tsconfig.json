{
  "compilerOptions": {
    "target": "ES2022",
    "lib": ["ES2022", "DOM"],
    "module": "ESNext",
    "moduleResolution": "node",
    "rootDir": "./",
    "outDir": "./dist",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "types": ["node"],
    "downlevelIteration": true,
    "sourceMap": true
  },
  "ts-node": {
    "esm": true,
    "transpileOnly": true,
    "files": true
  },
  "include": ["AI_decision/**/*.ts", "Interactions/**/*.ts"],
  "exclude": ["node_modules", "dist", "UI"]
}